{
    "name": "discord-bot-generator",
    "version": "2.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "start": "node ./dist/index.js",
        "dev": "tsx watch ./src/index.ts",
        "build": "tsc",
        "format": "prettier --write .",
        "prepare": "husky",
        "web": "tsx watch ./src/web/server.ts",
        "web:build": "tsc && node ./dist/web/server.js"
    },
    "dependencies": {
        "@supabase/supabase-js": "^2.49.1",
        "body-parser": "^1.20.3",
        "colors": "^1.4.0",
        "commandkit": "^0.1.10",
        "cors": "^2.8.5",
        "discord.js": "^14.19.3",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "moment": "^2.30.1"
    },
    "devDependencies": {
        "@types/colors": "^1.1.3",
        "@types/cors": "^2.8.19",
        "@types/express": "^4.17.23",
        "@types/moment": "^2.11.29",
        "@types/node": "^22.14.1",
        "husky": "^9.1.7",
        "lint-staged": "^16.1.0",
        "prettier": "^3.5.3",
        "tsx": "^4.19.3",
        "typescript": "^5.8.3"
    },
    "lint-staged": {
        "*.{js,md}": "prettier --write"
    }
}
